{
  "name": "datenkraft/monolog-formatter",
  "description": "Datenkraft Monolog JSON Formatter",
  "homepage": "https://github.com/datenkraft/monolog-formatter",
  "type": "library",
  "keywords": [
    "Google",
    "Tracing",
    "Logging",
    "Error Reporting",
    "Formator Error",
    "log",
    "logging"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Datenkraft GmbH",
      "email": "office@datenkraft.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "monolog/monolog": "^3.3",
    "datenkraft/monolog-gke-formatter": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2.1",
    "squizlabs/php_codesniffer": "^3.7.2"
  },
  "autoload": {
    "psr-4": {
      "Datenkraft\\MonologFormatter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
